        <div class="row row-cols-lg-4 row-cols-md-2">
          <div class="col-lg col-12">
        <div class="card card-border mb-4">
          <div class="card-body">
            <span class="api-text">POST</span>
            <h4 class="api-title p-color">HTTP Method</h4>
          </div>
        </div>
      </div><!-- ./col -->
      <div class="col-lg col-12">
        <div class="card card-border mb-4">
          <div class="card-body">
            <span class="api-text">{{ site["url"] }}/api/v2</span>
            <h4 class="api-title s-color">API Url</h4>
          </div>
        </div>
      </div><!-- ./col -->
	        <div class="col-lg col-12">
        <div class="card card-border mb-4">
          <div class="card-body">
            <span class="api-text text-white">Get on the <a href="/account" style="text-decoration: none;">Account Page</a></span>
            <h4 class="api-title i-color">API</h4>
          </div>
        </div>
      </div><!-- ./col -->
	        <div class="col-lg col-12">
        <div class="card card-border mb-4">
