              <option value="1">Default</option>
            </select>
          </div>
        </div>
      </div>
      <div class="card-body">
        <div class="row">
          <div class="col-md-3 mb-4">
            <h6>key</h6>
            <p>Your API key</p>
          </div>
          <div class="col-md-3 mb-4">
            <h6>action</h6>
            <p>add</p>
          </div>
          <div class="col-md-3 mb-4">
            <h6>service</h6>
            <p>Service ID</p>
          </div>
          <div class="col-md-3 mb-4">
            <h6>link</h6>
            <p>Link to page</p>
          </div>
          <div class="col-md-3 mb-4">
            <h6>usernames</h6>
            <p>Usernames list separated by \r\n or \n</p>
          </div>
        </div>
      </div>
    </div>
  </section>
  <section class="api-section">
    <pre class="code card card-border card-body">{
    "order": 23501
}</pre>
  </section>
