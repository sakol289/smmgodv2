              </div>
			  <input type="hidden" name="_csrf" value="chL1fLZ1-KGfO0EFpWbhhOEkOgBeIu5aIxF176nc4yEdZpkd-TO9y-tZNTTJK6v-slN_RxVLljERRDuLm-_XUA==">
              <button type="submit" class="orange-btn">Change Password</button>
			  </form>
            </div>
          </div>
	  
	  
	  
	  
	  
	 
        

    

     <div class="col-lg-6">
            <div class="panel-box v2 pt-5 mb-3">
              <div class="input-wrap">
                <strong>Timezone Change
                </strong>
				<form  action="account/timezone" method="post">
                <div class="input-item">
                  <span>
                    <svg width="25" height="25" viewBox="0 0 25 25" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path
                        d="M12.1094 0C5.41992 0 0 5.41992 0 12.1094C0 18.7988 5.41992 24.2188 12.1094 24.2188C18.7988 24.2188 24.2188 18.7988 24.2188 12.1094C24.2188 5.41992 18.7988 0 12.1094 0ZM14.8975 17.0947L10.5908 13.9648C10.4395 13.8525 10.3516 13.6768 10.3516 13.4912V5.27344C10.3516 4.95117 10.6152 4.6875 10.9375 4.6875H13.2812C13.6035 4.6875 13.8672 4.95117 13.8672 5.27344V11.9971L16.9678 14.2529C17.2314 14.4434 17.2852 14.8096 17.0947 15.0732L15.7178 16.9678C15.5273 17.2266 15.1611 17.2852 14.8975 17.0947Z"
                        fill="white" />
                    </svg>
                  </span>
				  <select style="background: transparent;
    border: none;width: 100%" name="timezone" id="timezone">
        

                 {% for timezone in timezones %}
                 <option {% if timezone["timezone"] == user['timezone'] %} selected {% endif %} value="{{ timezone["timezone"] }}">{{ timezone["label"] }}</option>
                 {% endfor %}
             </select>
           </div>
              </div>
			
              <button type="submit" class="orange-btn">Change</button>
			  </form>
       </div>
{% if site['languages']|length > 1 %}
     <div class="panel-box v2 pt-5 mb-3">
              <div class="input-wrap">
                <strong>Language Change
                </strong>
				<form  action="/account/change_lang" method="post">
                <div class="input-item">
                  <span>
                    <svg width="26" height="20" viewBox="0 0 26 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path
