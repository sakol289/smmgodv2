         jQuery(".viewmoney").click(function() {
            if (balance == 1) balance = 2;
            else balance = 1;
            if (balance == 2) {
               $(".viewmoney").each(function() {
                  $(this).html("{{ currency["symbol"]  }} {{ (user["balance"]*currency["value"]) |round(2, 'floor') }} &nbsp;<i class='far fa-eye-slash' style='cursor: pointer;'></i>");
               });
               $(".viewspent").each(function() {
                  $(this).html("{{ currency["symbol"]  }} {{ (user["spent"]*currency["value"]) |round(2, 'floor') }} ");
               });
            } else {
               $(".viewmoney").each(function() {
                  $(this).html("****&nbsp;<i class='far fa-eye' style='cursor: pointer;'></i>");
               });
               $(".viewspent").each(function() {
                  $(this).html("$****");
               });
            }
            $.get('/changevariable/custom_variable_6?value=' + balance, function() {});
         });
</script>




